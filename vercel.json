{
  "version": 2,
  "buildCommand": "pip install -r requirements.txt",
  "functions": {
    "api/index.py": {
      "memory": 1024,
      "maxDuration": 120,
      "runtime": "python3.11"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],

}
